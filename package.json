{
  "name": "web3mail-usecase-demo",
  "private": true,
  "version": "0.1.5",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.ts\"",
    "check-format": "prettier --check \"src/**/*.ts\"",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:local": "cross-env BASE_URL=http://localhost:5173 npm run cypress:run -- --config chromeWebSecurity=false",
    "test:dev": "cross-env BASE_URL=https://dev-web3mail.iex.ec/protectedData npm run cypress:run",
    "test:prod": "cross-env BASE_URL=https://web3mail.iex.ec/protectedData npm run cypress:open",
    "test:open-local": "cross-env BASE_URL=http://localhost:5173 npm run cypress:open -- --config chromeWebSecurity=false",
    "test:open-dev": "cross-env BASE_URL=https://dev-web3mail.iex.ec/protectedData npm run cypress:open"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/space-mono": "^4.5.12",
    "@iexec/dataprotector": "^0.1.2",
    "@iexec/react-ui-kit": "github:iExecBlockchainComputing/react-ui-kit#semver:^2.0.2",
    "@iexec/web3mail": "^0.5.1",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.13.4",
    "@mui/x-data-grid": "^6.0.4",
    "@reduxjs/toolkit": "^1.9.5",
    "@wagmi/connectors": "^0.3.10",
    "@web3modal/ethereum": "^2.2.2",
    "@web3modal/react": "^2.2.2",
    "ethers": "^5.7.2",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "wagmi": "^0.12.16"
  },
  "devDependencies": {
    "@ethersproject/experimental": "^5.7.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "cypress": "^12.3.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "typescript": "^5.0.2",
    "vite": "^4.3.2"
  }
}
